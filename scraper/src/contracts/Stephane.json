{
  "itemSelector": ".roomlistings .room",
  "pageQuery": "page",
  "scrapeAfterLoading": true,
  "attributes": {
    "name": { "type": "text", "selector": ".title-wrap"},
    "price": { "type": "digit", "selector": ".card-right"},
    "size": { "type": "size", "selector": ".title-wrap"},
    "link": { "type": "link", "selector": ".title-wrap", "attribute": "href"},
    "photo": { "type": "background-image", "selector": ".img", "attribute": "style"}
  }
}
