{
  "itemSelector": "[data-product-id]",
  "attributes": {
    "name": {
      "type": "text",
      "selector": ".item-products_pieces"
    },
    "description": {
      "type": "text",
      "selector": ".item-products_address"
    },
    "price": {
      "type": "number",
      "selector": ".item-products_price"
    },
    "size": {
      "type": "size",
      "selector": ".item-products_pieces"
    },
    "link": {
      "type": "stephane-link",
      "attribute": "data-url"
    },
    "photo": {
      "type": "background-image",
      "selector": ".slides-image.webresize",
      "attribute": "style"
    }
  }
}