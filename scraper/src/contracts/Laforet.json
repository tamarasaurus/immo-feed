{
    "itemSelector": ".properties__list .property-card",
    "pageQuery": "page",
    "attributes": {
      "name": { "type": "text", "selector": ".property-card__title" },
      "price": { "type": "digit", "selector": ".property-card__price"},
      "description": { "type": "text", "selector": ".property-card__infos .property-card__feature:nth-child(2)"},
      "size": { "type": "size", "selector": ".property-card__infos .property-card__feature:nth-child(1)"},
      "link": { "type": "link", "selector": "a", "attribute": "href"},
      "photo": { "type": "link", "selector": ".property-card__photo", "attribute": "src"}
    }
  }
