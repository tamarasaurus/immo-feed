{
  "itemSelector": ".resultsListContainer .sideListItemContainer",
  "puppeteer": true,
  "waitForPageLoadSelector": ".resultsListContainer",
  "attributes": {
    "name": {
      "type": "text",
      "selector": ".generatedTitleWithHighlight"
    },
    "description": {
      "type": "text",
      "selector": ".cityAndDistrict"
    },
    "price": {
      "type": "number",
      "selector": ".thePrice"
    },
    "size": {
      "type": "size",
      "selector": ".generatedTitleWithHighlight"
    },
    "link": {
      "type": "link",
      "selector": ".detailedSheetLink",
      "attribute": "href"
    },
    "photo": {
      "type": "link",
      "selector": ".searchItemPhoto",
      "attribute": "src"
    }
  }
}