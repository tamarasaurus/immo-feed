{
    "itemSelector": "article[data-id]",
    "scrapeAfterLoading": true,
    "pageQuery": "page",
    "attributes": {
      "name": { "type": "text", "selector": ".generatedTitleWithHighlight" },
      "description": { "type": "text", "selector": ".cityAndDistrict" },
      "size": { "type": "size", "selector": ".generatedTitleWithHighlight" },
      "link": { "type": "link", "selector": "a.detailedSheetLink", "attribute": "href" },
      "price": { "type": "price", "selector": ".thePrice" },
      "photo": { "type": "link", "selector": ".searchItemPhoto", "attribute": "src"}
    }
  }
