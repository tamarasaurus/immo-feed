{
  "itemSelector": "[data-test=\"sl.card-container\"]",
  "puppeteer": true,
  "attributes": {
    "name": {
      "type": "text",
      "selector": "[data-test=\"sl.title\"]"
    },
    "description": {
      "type": "text",
      "selector": "[data-test=\"sl.classified-description\"]"
    },
    "price": {
      "type": "number",
      "selector": "[data-test=\"sl.price-container\"]"
    },
    "size": {
      "type": "size",
      "selector": "[data-test=\"sl.tags\"] li:nth-child(3)"
    },
    "link": {
      "type": "link",
      "selector": ".classified-link",
      "attribute": "href"
    }
  }
}