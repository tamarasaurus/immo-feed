{
  "itemSelector": "[class^='Page__Wrap']",
  "pageQuery": "LISTING-LISTpg",
  "attributes": {
    "name": { "type": "text", "selector": "[class^='ContentZone__Title']"},
    "description": { "type": "text", "selector": "[class^='Card__Description']"},
    "price": { "type": "digit", "selector": "[class^='Price__Label']"},
    "size": { "type": "size", "selector": "[class^='ContentZone__Tags'] li:nth-child(3)"},
    "link": { "type": "link", "selector": "[class^='CoveringLink']", "attribute": "href"}
  }
}
