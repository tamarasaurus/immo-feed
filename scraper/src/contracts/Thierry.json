{
  "itemSelector": ".vignette-annonces .vignette-mf",
  "attributes": {
    "name": {
      "type": "text",
      "selector": ".card-titre"
    },
    "description": {
      "type": "text",
      "selector": ".accroche"
    },
    "price": {
      "type": "number",
      "selector": ".prix"
    },
    "photo": {
      "type": "link",
      "selector": "img",
      "attribute": "src"
    },
    "link": {
      "type": "link",
      "selector": ".card-image a",
      "attribute": "href"
    }
  }
}